{
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "Tags",
  "description": "Tags (or keywords) help users discover your dataset; please include terms that would be used by technical and non-technical users.",
  "type": "object",
  "required": [
    "title",
    "identifier"
  ],
  "properties": {
    "title": {
      "title": "Title",
      "type": "string"
    },
    "identifier": {
      "type": "string",
      "title": "Identifier"
    },
    "created": {
      "type": "string",
      "title": "Created",
      "format": "date-time"
    },
    "modified": {
      "type": "string",
      "title": "Modified",
      "format": "date-time"
    },
    "icon": {
      "type": "string",
      "title": "icon"
    }
  }
}
