{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Category",
  "description": "Main thematic category of the dataset.",
  "type": "object",
  "required": [
    "title",
    "identifier",
    "created",
    "modified"
  ],
  "properties": {
    "title": {
      "title": "Title",
      "type": "string"
    },
    "identifier": {
      "type": "string",
      "title": "Identifier"
    },
    "created": {
      "type": "string",
      "title": "Created",
      "format": "date-time"
    },
    "modified": {
      "type": "string",
      "title": "Modified",
      "format": "date-time"
    },
    "icon": {
      "type": "string",
      "title": "icon"
    }
  }
}
